{
  "name": "apolloview",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "dev": "expo start",
    "dev:ios": "expo start --ios",
    "dev:web": "expo start --web",
    "prebuild": "expo prebuild",
    "build": "eas build --platform all --profile development",
    "build:local": "EXPO_USE_COMMUNITY_AUTOLINKING=1 expo run:ios --device",
    "build:ios": "eas build --platform ios --profile development",
    "build:web": "eas build --platform web --profile development",
    "build:preview": "eas build --platform ios --profile preview",
    "build:production": "eas build --platform ios --profile production",
    "build:sim": "EXPO_USE_COMMUNITY_AUTOLINKING=1 expo run:ios --device 'iPhone 16 Pro'",
    "submit:preview": "eas submit --platform ios --profile preview",
    "submit:production": "eas submit --platform ios --profile production",
    "lint": "expo lint",
    "android": "expo run:android",
    "ios": "EXPO_USE_COMMUNITY_AUTOLINKING=1 expo run:ios"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "expo": "~54.0.9",
    "expo-auth-session": "^7.0.8",
    "expo-camera": "^17.0.8",
    "expo-constants": "~18.0.9",
    "expo-font": "~14.0.8",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.8",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.7",
    "expo-secure-store": "^15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.7",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1",
    "@react-native-community/cli": "^20.0.2"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.2"
  },
  "private": true
}
