{
  "name": "apollo",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
		"check-types": "turbo run check-types",
		"test": "turbo run test --filter=apollo --filter=cms"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
		"turbo": "^2.5.6",
		"typescript": "5.9.2",
		"vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.2.20",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "^3.890.0",
    "tailwindcss-radix": "^4.0.2"
  }
}
